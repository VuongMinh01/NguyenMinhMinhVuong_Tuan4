package com.jetbrains.SpringDataJPA;

import org.springframework.boot.CommandLineRunner;
import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;
import org.springframework.context.annotation.Bean;

import entity.ChuyenBay;
import repository.ChuyenBayRepository;

@SpringBootApplication
public class SpringDataJpaApplication {

	public static void main(String[] args) {
		SpringApplication.run(SpringDataJpaApplication.class, args);
	}
	
	@Bean
	public CommandLineRunner run(ChuyenBayRepository repository) {
		return (args ->{
			System.out.println(repository.findChuyenBaysDaLat("DAD"));
		});
	}

}
